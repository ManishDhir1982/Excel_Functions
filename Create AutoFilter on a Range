Function fn_Create_Filter(in_Workbook As Workbook, in_Sheet As Worksheet, in_Range As Range)

Dim rng_Filter_Range As Range

Set rng_Filter_Range = in_Sheet.Range(in_Range.Address)

If in_Sheet.AutoFilterMode = True Then
    rng_Filter_Range.AutoFilter
End If

rng_Filter_Range.AutoFilter

End Function
